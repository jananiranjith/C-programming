
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int a;
    scanf("%d",&a);
    if(a>0){
        printf("+%d",a);
    }
    else{
        printf("%d",a);
    }
    
    return 0;
}
