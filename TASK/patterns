1. star pattern nested loop

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n;

    scanf("%d",&n);
    for(int i=1;i<=n;i++) {
        for(int j=1;j<=n;j++) {
            printf("*");
        }
        printf("\n");
       
    }
    return 0;
}

2. diff rows and columns 

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int a,b;
    scanf("%d %d",&a,&b);
    for(int i=1;i<=a;i++) {
        for(int j=1;j<=b;j++) {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}

3.right triangle 
int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int n;
    scanf("%d",&n);
    for(int i=1;i<=n;i++) {
        for(int j=1;j<=i;j++) {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
4. inverted right triangle

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int n;
    scanf("%d",&n);
    for(int i=1;i<=n;i++) {
        for(int j=n;j>=i;j--) {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}

5. rigt triangle in numbers 

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n;
    scanf("%d",&n);
    for(int i=1;i<=n;i++) {
        for(int j=1;j<=i;j++) {
            printf("%d",j);
        }
        printf("\n");
    }
    return 0;
}
